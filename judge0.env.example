# Judge0 Environment Configuration
# Copy this file to .env in the same directory as docker-compose.judge0.yml

# Database Configuration
POSTGRES_DB=judge0
POSTGRES_USER=judge0
POSTGRES_PASSWORD=YourSecurePassword123_ChangeThis

# Redis Configuration (leave empty for no password)
REDIS_PASSWORD=YourRedisPassword123_ChangeThis

# Judge0 Secret Key (generate a strong random key)
SECRET_KEY_BASE=your_secret_key_base_here_generate_a_long_random_string_64_chars_min

# Network Configuration
JUDGE0_HOST=localhost
JUDGE0_PORT=2358

# Security Settings
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173
ALLOWED_HEADERS=Content-Type,Authorization

# Performance Settings
MAX_WORKERS=2
WORKER_INTERVAL=0.1
MAX_QUEUE_SIZE=100

# Execution Limits
MAX_CPU_TIME=15
MAX_WALL_TIME=20
MAX_MEMORY=128000
MAX_STACK=64000
MAX_PROCESSES=30 